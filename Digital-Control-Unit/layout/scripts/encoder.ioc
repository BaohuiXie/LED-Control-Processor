(globals
    version = 3
    io_order = default
    space = 6
)
(iopin
    (left
	(pin name="balanceCLK"	 layer=5 width=0.1400 depth=0.1400 offset=1.0)
	(pin name="REF4Bits[0]"	 layer=5 width=0.1400 depth=0.1400 )
	(pin name="REF4Bits[1]"	 layer=5 width=0.1400 depth=0.1400 )
	(pin name="REF4Bits[2]"	 layer=5 width=0.1400 depth=0.1400 )
	(pin name="REF4Bits[3]"	 layer=5 width=0.1400 depth=0.1400 )
	(pin name="reData"	 layer=5 width=0.1400 depth=0.1400 )
	(pin name="clk"	 layer=5 width=0.1400 depth=0.1400 )
	(pin name="globalReset"	 layer=5 width=0.1400 depth=0.1400 )
    )
    (right
	(pin name="out"	 layer=6 width=0.1400 depth=0.1400 offset=1.0)
    )
)
