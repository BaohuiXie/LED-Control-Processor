    (right
	(pin name="recoveredData"	 layer=6 width=0.300 depth=0.1400 offset=1.0)
	(pin name="recoveredCLK"	 layer=6 width=0.300 depth=0.1400)
	(pin name="balancedCLK"	 layer=6 width=0.300 depth=0.1400)
    )
